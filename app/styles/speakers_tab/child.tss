'#shareButton[platform=ios]' : {
  title: L('shareButton')
}
'#shareMenuItem[platform=android]' : {
  icon: Ti.Android.R.drawable.ic_menu_share
}
'#containnerScrollView' : {
  scrollType: 'vertical'
}
'.horizontalView' : {
  width: Ti.UI.SIZE
}
'#iconsView' : {
  top: 180,
  height: 40,
  width: Ti.UI.SIZE,
  layout: 'horizontal'
}
'#featuredImageView' : {
  left: 0,
  right: 0,
  top: 0,
  height: 200
}
'#featuredImageView[platform=android]' : {
  scrollingEnabled: false,
  contentWidth: '100%',
  scrollType: 'vertical'
}
'#containnerView' : {
  textAlign: Alloy.CFG.tss.common.leftTextAlign,
  top: 200,
  left: 10,
  right: 10,
  backgroundColor: '#ffffff',
  layout: 'vertical'
}
'#nameLabel' : {
  top: 30,
  font: {
    fontSize: 22
  }
}
'#titleLabel' : {
  color: 'gray',
  font: {
    fontSize: 14
  }
}
'#infoImageView' : {
  width: 29,
  image: '/images/info_ic.png',
  left: Alloy.CFG.tss.speakers.child.infoImageView.left,
  right: Alloy.CFG.tss.speakers.child.infoImageView.right
}
'#aboutLabel' : {
  text: L('about'),
  color: Alloy.CFG.style.mainColor,
  left: Alloy.CFG.tss.speakers.child.aboutLabel.left,
  right: Alloy.CFG.tss.speakers.child.aboutLabel.right
}
'#bioLabel' : {
textAlign: Alloy.CFG.tss.common.leftTextAlign,
  left: 3,
  top: 10,
